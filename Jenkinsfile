pipeline {
    agent { docker { image 'golang' } }
    stages {
        stage('build') {
            steps {
                 bash '''#!/bin/bash
                        sleep 10
                        echo "hello world"
                 '''
            }
        }
        stage('Setting the variables values') {
            steps {
                 bash '''#!/bin/bash
                        sleep 10
                        echo "hello world"
                 '''
            }
        }
    }
}
